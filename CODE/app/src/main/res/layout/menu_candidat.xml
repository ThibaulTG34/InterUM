<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="390dp"
        android:layout_height="138dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/SeeOffer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="#FF0000"
        android:text="Voir les offres"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <Button
        android:id="@+id/SeeCandidatures"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="112dp"
        android:layout_marginTop="27dp"
        android:backgroundTint="#FF0000"
        android:text="Mes candidatures"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SeeOffer" />

    <Button
        android:id="@+id/SeeJobs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:backgroundTint="#FF1100"
        android:text="Mes emplois"
        app:layout_constraintTop_toBottomOf="@+id/SeeCandidatures"
        tools:layout_editor_absoluteX="137dp" />

    <Button
        android:id="@+id/SeeAlerts"
        android:layout_width="188dp"
        android:layout_height="57dp"
        android:layout_marginStart="111dp"
        android:layout_marginTop="29dp"
        android:backgroundTint="#FF1100"
        android:text="Mes notifications"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SeeJobs" />

    <Button
        android:id="@+id/instantCandidature"
        android:layout_width="188dp"
        android:layout_height="57dp"
        android:layout_marginStart="111dp"
        android:layout_marginTop="30dp"
        android:backgroundTint="#FF1100"
        android:text="Candidature SpontanÃ©e"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SeeAlerts" />
</androidx.constraintlayout.widget.ConstraintLayout>